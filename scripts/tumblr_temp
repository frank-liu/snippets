var toast_translations = {
// Like
"%1$s liked your post": '%1$s liked your post',
"%1$s liked your photo": '%1$s liked your photo',
"%1$s liked your photoset": '%1$s liked your photoset',
"%1$s liked your panorama": '%1$s liked your panorama',
"%1$s liked your quote": '%1$s liked your quote',
"%1$s liked your link": '%1$s liked your link',
"%1$s liked your chat": '%1$s liked your chat',
"%1$s liked your video": '%1$s liked your video',
"%1$s liked your audio post": '%1$s liked your audio post',
// Reblog
"%1$s reblogged your post": '%1$s reblogged your post',
"%1$s reblogged your photo": '%1$s reblogged your photo',
"%1$s reblogged your photoset": '%1$s reblogged your photoset',
"%1$s reblogged your panorama": '%1$s reblogged your panorama',
"%1$s reblogged your quote": '%1$s reblogged your quote',
"%1$s reblogged your link": '%1$s reblogged your link',
"%1$s reblogged your chat": '%1$s reblogged your chat',
"%1$s reblogged your video": '%1$s reblogged your video',
"%1$s reblogged your audio post": '%1$s reblogged your audio post',
// Reblog with diff
"%1$s reblogged your post and added:": '%1$s reblogged your post and added:',
"%1$s reblogged your photo and added:": '%1$s reblogged your photo and added:',
"%1$s reblogged your photoset and added:": '%1$s reblogged your photoset and added:',
"%1$s reblogged your panorama and added:": '%1$s reblogged your panorama and added:',
"%1$s reblogged your quote and added:": '%1$s reblogged your quote and added:',
"%1$s reblogged your link and added:": '%1$s reblogged your link and added:',
"%1$s reblogged your chat and added:": '%1$s reblogged your chat and added:',
"%1$s reblogged your video and added:": '%1$s reblogged your video and added:',
"%1$s reblogged your audio post and added:": '%1$s reblogged your audio post and added:',
// Answer
"%1$s answered your question": '%1$s answered your question',
// Reply or photo reply
"%1$s replied to your post": '%1$s replied to your post',
"%1$s replied to your photo": '%1$s replied to your photo',
"%1$s replied to your photoset": '%1$s replied to your photoset',
"%1$s replied to your panorama": '%1$s replied to your panorama',
"%1$s replied to your quote": '%1$s replied to your quote',
"%1$s replied to your link": '%1$s replied to your link',
"%1$s replied to your chat": '%1$s replied to your chat',
"%1$s replied to your video": '%1$s replied to your video',
"%1$s replied to your audio post": '%1$s replied to your audio post',
// Follow
"%1$s started following %2$s": '%1$s started following %2$s',
"%1$s started following you": '%1$s started following you',
// Ask
"Sent to %1$s": 'Sent to %1$s'
};

jQuery(document).ready(function($){

// Initialize Toaster
Tumblr.Toaster = new Tumblr.Toaster({
el: '#toaster'
});

// Add translations to ToastKit
Tumblr.ToastKit.add_translations(toast_translations);

});
